<mx:Panel xmlns:mx="http://www.adobe.com/2006/mxml" layout="absolute" width="779" height="466" title="Configuración Vacunas" borderColor="#15AD8F">
	<mx:Script>
		<![CDATA[
			//=======================================================================
			// FECHA: CREACIÓN: 27 Octubre
			// AUTOR: Camilo Verdugo
			// Modulo de mantencion de vacunas del sistema
			//=======================================================================
			import mx.core.Application;
			import mx.collections.ArrayCollection;   
			
			[Bindable]private var vacunas:ArrayCollection = new ArrayCollection([
		          {nombre:"Parvo Corona"},
		          {nombre:"Sextuple"},
		          {nombre:"Coronavirus"},
		          {nombre:"Antirrabica"},
		          {nombre:"Octuple"},
		          {nombre:"Triple Felina"},
		          {nombre:"Leucemia Felina"},
		          {nombre:"PIF"},
	        ]);
			
	
			/**
		 	 * 	@author  "Camilo Verdugo"
			 * 	@Fecha  27 Octubre
			 *  @Descripcion metodo que retorna al panel de administacion, este es llamado
			 *  por el botton "Volver"
			 * */			
			public function cancelarVisulizar():void
			{	
				Application.application.setInvisible();
            	Application.application.panel2.setVisible(true,false);
            	Application.application.AdmOpciones1.setVisible(true,false);
			}
			
		]]>
	</mx:Script>
	<mx:DataGrid x="34.95" y="58" width="286.15" dataProvider="{vacunas}">
		<mx:columns>
			<mx:DataGridColumn headerText="Nombre" dataField="nombre"/>
		</mx:columns>
	</mx:DataGrid>
	<mx:Label x="35" y="32" text="Vacunas disponibles" width="154" fontWeight="bold"/>
